# Local backend environment variables (DO NOT put real secrets in git)
ODDS_API_KEY="YOUR_ODDS_API_KEY"
PROJECTIONS_PROVIDER="odds_api"
SPORTSBOOK_CACHE_TTL_SECONDS="1800"

# Optional: prefer certain books when multiple are returned (Odds API)
ODDS_API_BOOKMAKERS="draftkings,fanduel"

# Optional: allow nba_api fallback (can timeout/be blocked)
ENABLE_RECENT_GAMES_FALLBACK="0"
